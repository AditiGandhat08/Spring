<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>

<class name="com.pojo.FoodPojo" table="foodplaza_hibernate">
<id name="foodid" column="FoodId">
<generator class="increment"></generator>
</id>
<property name="foodname" column="Name"></property>
<property name="foodtype" column="Type"></property>
<property name="foodcategory" column="Category"></property>
<property name="price" column="Price"></property>
</class>

</hibernate-mapping>